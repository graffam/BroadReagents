<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Date Posted</th>
      <th>Date Needed</th>
      <th>Status</th>
      <th>Comments</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.user_id %></td>
        <td><%= order.created_at %> </td>
        <td><%= order.needed_by %> </td>
        <td><%= order.status %> </td>
        <td><%= order.comment %> </td>
      </tr>
    <% end %>
</tbody>
</table>
